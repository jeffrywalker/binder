{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Attach",
            "type": "cppvsdbg",
            "request": "attach",
        },
        {
            "name": "(Windows) Launch: SimpleClass",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/llvm-13.0.0/llvm-13.0.0.Windows-10-10.0.19044-SP0.debug/bin/binder.exe",
            "args": [
                "--root-module",
                "test_class",
                "--prefix",
                "bash_bindings",
                "--bind",
                "testers",
                "all_bash_includes.hpp",
                "--",
                "-Iinclude",
                "-DNDEBUG"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/examples/simple_class",
            "environment": [],
            "console": "integratedTerminal"
        }
    ]
}